  "scripts": {
    "postinstall": "netlify-lambda install src/lambda",
    "build": "webpack --config ./src/build/webpack.config.js",
    "build:prod": "webpack --config ./src/build/webpack.config.js --env production --node-env production",
    "serve": "nodemon ./src/server/task-serve",
    "serve:lambda": "netlify-lambda serve src/lambda",
    "dev": "concurrently -n build,lamda,serve 'yarn:build --watch' yarn:serve:lambda yarn:serve -k",
    "generate": "node ./src/server/task-generate",
    "start": "webpack --config ./src/build/webpack.config.js && node ./src/server/task-serve",
    "deploy": "yarn build:prod && yarn generate",
    "test": "jest"
  },
  "dependencies": {
